    <!-- Shopping Cart Section Begin -->
    <section class="shopping-cart spad">
        <div class="container mt-5">
            <h6 class="pr-5 checkout__title">Order History</h6>
            <div class="shopping__cart__table">
                <table class="shopping__cart__table">
                    <thead class="thead-dark">
                        <tr>
                            <th>Product Images</th>
                            <th>Product Name</th>
                            <th>Ordered on</th>
                            <th>status</th>
                            <th>Total</th>
                            <th>Method</th>
                            <th>Manage</th>
                        </tr>
                    </thead>
                    <% if (typeof orderedProducts !=='undefined' && orderedProducts.length> 0) { %>
                        <tbody>
                            <% orderedProducts.reverse(); %>
                            <% for (let i=0; i < orderedProducts.length; i++) { %>
                                <tr>
                                    <td>
                                        <% for (let j=0; j < Math.min(3, orderedProducts[i].items.length); j++) { %>
                                            <img src="/uploads/<%= orderedProducts[i].items[j].productID.image[0] %>"
                                                class="img-fluid mb-2"
                                                alt="<%= orderedProducts[i].items[j].productID.productName %>"
                                                style="width: 80px; height: 80px; object-fit: cover; border-radius: 5%;">
                                            <% } %>
                                    </td>
                                    <td class="product__cart__item__text">
                                        <% for (let j=0; j < Math.min(3, orderedProducts[i].items.length); j++) { %>
                                            <div class="d-flex flex-column align-items-center">
                                                <p class="mb-0">
                                                    <%= orderedProducts[i].items[j].productID.productName %>
                                                </p>
                                            </div>
                                            <% } %>
                                    </td>
                                    <td class="quantity__item">
                                        <%= orderedProducts[i].orderDate.toLocaleDateString() %>
                                    </td>
                                    <td>
                                        <%= orderedProducts[i].orderStatus%>
                                    </td>
                                    <td>
                                        â‚¹<%= orderedProducts[i].totalAmount %>
                                    </td>
                                    <td>
                                        <%= orderedProducts[i].paymentMethod %>
                                    </td>
                                    <td>
                                        <div class="d-flex justify-content-center align-items-center">
                                            <a href="/order-details?id=<%= orderedProducts[i]._id %>">
                                                <button class="btn btn-dark">View Details</button>
                                            </a>
                                        </div>
                                    </td>
                                </tr>
                                <% } %>
                        </tbody>
                        <% } else { %>
                            <tr>
                                <td colspan="8" class="text-center text-primary pt-5 pb-5">
                                    <h3>No ordered products found.</h3>
                                    <div class="d-flex justify-content-center align-items-center mt-3">
                                        <a href="/shop">
                                            <button class="btn primary-btn" style="color: white;"> Shop now !</button>
                                        </a>
                                    </div>
                                </td>
                            </tr>

                            <% } %>
                </table>
            </div>
        </div>
    </section>
    <!-- Shopping Cart Section End -->



